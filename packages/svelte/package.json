{
    "name": "@zeejs/svelte",
    "version": "0.1.3",
    "description": "Svelte layering",
    "author": "Ido Rosenthal",
    "main": "./cjs/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "types": "esm/index.d.ts",
    "svelte": "esm/index.js",
    "scripts": {
        "clean": "rimraf ./cjs ./esm ./dist",
        "build": "ts-build ./src --cjs --esm && node build-svelte.js",
        "test": "node -r @ts-tools/node/r -r tsconfig-paths/register ./test/setup.ts",
        "prepack": "yarn clean && yarn build",
        "demo": "webpack-dev-server"
    },
    "dependencies": {
        "@zeejs/browser": "^0.1.0",
        "@zeejs/core": "^0.1.0"
    },
    "devDependencies": {
        "svelte": "^3.24.0",
        "svelte-loader": "2.13.6"
    },
    "files": [
        "cjs",
        "esm",
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "repository": "https://github.com/idoros/zeejs/tree/master/packages/svelte"
}
