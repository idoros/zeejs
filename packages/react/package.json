{
    "name": "@zeejs/react",
    "version": "0.1.1",
    "description": "React layering",
    "author": "Ido Rosenthal",
    "main": "./cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "build": "ts-build ./src --cjs --esm",
        "test": "node -r @ts-tools/node/r -r tsconfig-paths/register ./test/setup.ts",
        "test:dev": "cross-env DEV=true PWDEBUG=1 node -r @ts-tools/node/r -r tsconfig-paths/register ./test/setup.ts",
        "prepack": "yarn build",
        "demo": "webpack-dev-server"
    },
    "dependencies": {
        "@zeejs/browser": "^0.1.0",
        "@zeejs/core": "^0.1.0"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "devDependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "files": [
        "cjs",
        "esm",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "repository": "https://github.com/idoros/zeejs/tree/master/packages/react"
}
